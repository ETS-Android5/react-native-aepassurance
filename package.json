  },
  "devDependencies": {
    "@babel/generator": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-preset-react-native": "5.0.2",
    "flow-typed": "^2.5.2",
    "jest": "26.0.1",
    "react": "^16.11.0",
    "react-native": "^0.62.2"
  },
  "jest": {
    "preset": "react-native",
